    %parti/s 3, 0, 2;
    %concati/vec4 0, 0, 1;
    %assign/vec4 v0x60684783ebd0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x60684782bd30;
T_1 ;
    %wait E_0x6068477f1b60;
    %load/vec4 v0x60684783ebd0_0;
    %parti/s 1, 3, 3;
    %store/vec4 v0x60684783eac0_0, 0, 1;
    %jmp T_1;
