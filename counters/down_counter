#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x57490ca6cba0 .scope module, "tb_down_counter" "tb_down_counter" 2 1;
 .timescale 0 0;
v0x57490ca7de60_0 .var "clk", 0 0;
v0x57490ca7df30_0 .net "count", 3 0, v0x57490ca7dc60_0;  1 drivers
v0x57490ca7e000_0 .var "reset", 0 0;
S_0x57490ca6cd30 .scope module, "uut" "down_counter" 2 7, 3 1 0, S_0x57490ca6cba0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x57490ca33490_0 .net "clk", 0 0, v0x57490ca7de60_0;  1 drivers
v0x57490ca7dc60_0 .var "count", 3 0;
v0x57490ca7dd40_0 .net "rst", 0 0, v0x57490ca7e000_0;  1 drivers
E_0x57490ca346a0 .event posedge, v0x57490ca7dd40_0, v0x57490ca33490_0;
    .scope S_0x57490ca6cd30;
T_0 ;
    %wait E_0x57490ca346a0;
    %load/vec4 v0x57490ca7dd40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x57490ca7dc60_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x57490ca7dc60_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x57490ca7dc60_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x57490ca6cba0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x57490ca7de60_0;
    %inv;
    %store/vec4 v0x57490ca7de60_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x57490ca6cba0;
T_2 ;
    %vpi_call 2 16 "$dumpfile", "down_counter.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57490ca6cba0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57490ca7de60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57490ca7e000_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57490ca7e000_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_down_counter.v";
    "down_counter.v";
