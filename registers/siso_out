#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b4d55f09420 .scope module, "tb_siso" "tb_siso" 2 1;
 .timescale 0 0;
v0x5b4d55f1be40_0 .var "clk", 0 0;
v0x5b4d55f1bf00_0 .var "reset", 0 0;
v0x5b4d55f1bfd0_0 .var "sin", 0 0;
v0x5b4d55f1c0d0_0 .net "sout", 0 0, v0x5b4d55f1bbd0_0;  1 drivers
S_0x5b4d55f095b0 .scope module, "uut" "siso" 2 5, 3 1 0, S_0x5b4d55f09420;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "sin";
    .port_info 3 /OUTPUT 1 "sout";
v0x5b4d55ecf490_0 .net "clk", 0 0, v0x5b4d55f1be40_0;  1 drivers
v0x5b4d55f1ba40_0 .net "reset", 0 0, v0x5b4d55f1bf00_0;  1 drivers
v0x5b4d55f1bb00_0 .net "sin", 0 0, v0x5b4d55f1bfd0_0;  1 drivers
v0x5b4d55f1bbd0_0 .var "sout", 0 0;
v0x5b4d55f1bc90_0 .var "temp", 3 0;
E_0x5b4d55ecfdc0 .event anyedge, v0x5b4d55f1bc90_0;
E_0x5b4d55ed0060 .event posedge, v0x5b4d55f1ba40_0, v0x5b4d55ecf490_0;
    .scope S_0x5b4d55f095b0;
T_0 ;
    %wait E_0x5b4d55ed0060;
    %load/vec4 v0x5b4d55f1ba40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5b4d55f1bc90_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5b4d55f1bc90_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x5b4d55f1bb00_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5b4d55f1bc90_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b4d55f095b0;
T_1 ;
    %wait E_0x5b4d55ecfdc0;
    %load/vec4 v0x5b4d55f1bc90_0;
    %parti/s 1, 3, 3;
    %store/vec4 v0x5b4d55f1bbd0_0, 0, 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5b4d55f09420;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5b4d55f1be40_0;
    %inv;
    %store/vec4 v0x5b4d55f1be40_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5b4d55f09420;
T_3 ;
    %vpi_call 2 9 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b4d55f1be40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b4d55f1bf00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b4d55f1bfd0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b4d55f1bf00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b4d55f1bfd0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b4d55f1bfd0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b4d55f1bfd0_0, 0, 1;
    %delay 30, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_siso.v";
    "siso.v";
