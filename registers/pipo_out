S_0x563393c7df20 .scope module, "tb_pipo" "tb_pipo" 2 1;
 .timescale 0 0;
v0x563393c8fee0_0 .var "clk", 0 0;
v0x563393c8ffa0_0 .var "d", 3 0;
v0x563393c90070_0 .net "q", 3 0, v0x563393c8fc80_0;  1 drivers
v0x563393c90170_0 .var "reset", 0 0;
S_0x563393c7e0b0 .scope module, "uut" "pipo" 2 6, 3 1 0, S_0x563393c7df20;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 4 "d";
    .port_info 3 /OUTPUT 4 "q";
v0x563393c44490_0 .net "clk", 0 0, v0x563393c8fee0_0;  1 drivers
v0x563393c8fba0_0 .net "d", 3 0, v0x563393c8ffa0_0;  1 drivers
v0x563393c8fc80_0 .var "q", 3 0;
v0x563393c8fd70_0 .net "reset", 0 0, v0x563393c90170_0;  1 drivers
E_0x563393c44f50 .event posedge, v0x563393c8fd70_0, v0x563393c44490_0;
    .scope S_0x563393c7e0b0;
T_0 ;
    %wait E_0x563393c44f50;
    %load/vec4 v0x563393c8fd70_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x563393c8fc80_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563393c8fba0_0;
    %assign/vec4 v0x563393c8fc80_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563393c7df20;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x563393c8fee0_0;
    %inv;
    %store/vec4 v0x563393c8fee0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x563393c7df20;
T_2 ;
    %vpi_call 2 11 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563393c8fee0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563393c90170_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x563393c8ffa0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563393c90170_0, 0, 1;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x563393c8ffa0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x563393c8ffa0_0, 0, 4;
    %delay 20, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
