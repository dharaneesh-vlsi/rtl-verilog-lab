#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c6d7dbfd760 .scope module, "tb_bidirectional" "tb_bidirectional" 2 1;
 .timescale 0 0;
v0x5c6d7dc10120_0 .var "clk", 0 0;
v0x5c6d7dc101e0_0 .var "dir", 0 0;
v0x5c6d7dc102b0_0 .net "q", 3 0, v0x5c6d7dc0fdc0_0;  1 drivers
v0x5c6d7dc103b0_0 .var "reset", 0 0;
v0x5c6d7dc10480_0 .var "sin", 0 0;
S_0x5c6d7dbfd8f0 .scope module, "uut" "bidirectional" 2 5, 3 1 0, S_0x5c6d7dbfd760;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "dir";
    .port_info 3 /INPUT 1 "sin";
    .port_info 4 /OUTPUT 4 "q";
v0x5c6d7dbfdaf0_0 .net "clk", 0 0, v0x5c6d7dc10120_0;  1 drivers
v0x5c6d7dc0fd00_0 .net "dir", 0 0, v0x5c6d7dc101e0_0;  1 drivers
v0x5c6d7dc0fdc0_0 .var "q", 3 0;
v0x5c6d7dc0feb0_0 .net "reset", 0 0, v0x5c6d7dc103b0_0;  1 drivers
v0x5c6d7dc0ff70_0 .net "sin", 0 0, v0x5c6d7dc10480_0;  1 drivers
E_0x5c6d7dbc3dc0 .event posedge, v0x5c6d7dc0feb0_0, v0x5c6d7dbfdaf0_0;
    .scope S_0x5c6d7dbfd8f0;
T_0 ;
    %wait E_0x5c6d7dbc3dc0;
    %load/vec4 v0x5c6d7dc0feb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5c6d7dc0fdc0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5c6d7dc0fd00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5c6d7dc0fdc0_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x5c6d7dc0ff70_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5c6d7dc0fdc0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5c6d7dc0ff70_0;
    %load/vec4 v0x5c6d7dc0fdc0_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5c6d7dc0fdc0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5c6d7dbfd760;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5c6d7dc10120_0;
    %inv;
    %store/vec4 v0x5c6d7dc10120_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5c6d7dbfd760;
T_2 ;
    %vpi_call 2 9 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c6d7dc10120_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c6d7dc103b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c6d7dc101e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c6d7dc10480_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c6d7dc103b0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c6d7dc101e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c6d7dc10480_0, 0, 1;
    %delay 30, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
